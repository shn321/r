# --- Create Data ---
data <- data.frame(
 Experience = c(1, 2, 3, 4, 5),
 Age = c(22, 25, 28, 30, 35),
 Salary = c(25000, 30000, 35000, 40000, 45000)
)
# --- Linear Regression Model ---
model <- lm(Salary ~ Experience + Age, data = data)
# --- Predict Salary ---
new <- data.frame(
 Experience = c(6, 7),
 Age = c(32, 35)
)
pred <- predict(model, new)
# --- Display Predicted Salaries ---
print("Predicted Salaries:")
print(pred)


--------------------------------------------------------------------------------------

data <- data.frame(
  Salary = c(50000, 60000, 75000, 80000, 95000, 110000, 120000, 130000),
  Experience = c(1, 2, 3, 4, 5, 6, 7, 8),
  Education = c(12, 14, 16, 16, 18, 20, 20, 22)
)

model <- lm(Salary ~ Experience + Education, data = data)

new_data <- data.frame(
  Experience = c(9, 10),
  Education = c(22, 24)
)

predicted_salaries <- predict(model, newdata = new_data)

cat("Predicted Salaries:\n")
print(predicted_salaries)
